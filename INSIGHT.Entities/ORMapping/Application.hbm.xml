<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="INSIGHT.Entities.Application" namespace="INSIGHT.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="INSIGHT.Entities.Application, INSIGHT.Entities" table="Application" lazy="false" >
    <id name="Id" column="Id" type="Int32">
      <generator class="identity" />
    </id>
    <property name="AppCode">
      <column name="AppCode" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="AppName">
      <column name="AppName" sql-type="nvarchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
