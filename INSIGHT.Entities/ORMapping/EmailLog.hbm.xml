<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="INSIGHT.Entities.EmailLog" namespace="INSIGHT.Entities" xmlns="urn:nhibernate-mapping-2.2">
  <class name="INSIGHT.Entities.EmailLog, INSIGHT.Entities" table="EmailLog" lazy="false" >
    <id name="Id" column="Id" type="Int64">
      <generator class="identity" />
    </id>
    <property name="EmailFrom" column="EmailFrom" type="string" ></property>
    <property name="EmailTo" column="EmailTo" type="string" ></property>
    <property name="EmailCC" column="EmailCC" type="string" ></property>
    <property name="EmailBCC" column="EmailBCC" type="string"></property>
    <property name="BCC_Count" column="BCC_Count" type="Int64"></property>

    <property name="Subject" column="Subject" type="string" ></property>
    <property name="Message" column="Message" type="string" ></property>
    <property name="Attachment" column="Attachment" type="string"></property>

    <property name="EmailDateTime" column="EmailDateTime" type="datetime"></property>
    <property name="IsSent" column="IsSent" type="bool"></property>
    <property name="Module" column="Module" type="string"></property>
  </class>
</hibernate-mapping>



