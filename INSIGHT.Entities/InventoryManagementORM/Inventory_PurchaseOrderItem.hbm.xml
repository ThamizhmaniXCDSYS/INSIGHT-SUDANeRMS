<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="INSIGHT.Entities.InventoryManagementEntities.Inventory_PurchaseOrderItem" namespace="INSIGHT.Entities" xmlns="urn:nhibernate-mapping-2.2">
