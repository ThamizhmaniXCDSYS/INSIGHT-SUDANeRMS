<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href='@Url.Content("../../Content/NewMaster.css")' rel="stylesheet" type="text/css" />
    <script type="text/javascript">        qm_create(0, false, 0, 500, false, false, false, false, false);</script>
    <!-- This script references optionally loads the MyCSSMenu visual interface, to run the menu stand alone remove the script.-->
    <script type="text/javascript">        if (window.name == "qm_launch_visual") { document.write('<scr' + 'ipt type="text/javascript" src="http://www.mycssmenu.com/qmv6/qm_visual.js"></scr' + 'ipt>') }</script>
    <script src='@Url.Content("../../Scripts/SiteMaster.js")' type="text/javascript"></script>
    <script src='@Url.Content("../../Scripts/jquery-1.7.2.js")' type="text/javascript"></script>
    <script src='@Url.Content("../../FusionCharts/FusionCharts.js")' type="text/javascript"></script>
</head>
<style>
    .tt table th
    {
        padding: 5px;
        text-align: left;
        color: black;
        border: none;
        font-size: 0.90em;
    }
    
    .tt table td
    {
        padding: 5px;
        border: none;
        color: Black;
        font-size: 0.85em;
    }
</style>
<body>
    <div class="page">
        <div class="header">
            <div id="title" style="height: 52px; width: 1010px; background-color: #B6B6B6;">
            </div>
            <div class="loginDisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div class="gradient" style="height: 10px; width: 950px; background-color: Gray;">
            </div>
            <div id="Div1" style="height: 3px; width: 960px; background-color: white;">
            </div>
        </div>
        <div id="menu">
                <ul id="qm0" class="qmmc">
                    <li><a href="Home" class="qmparent">Home</a></li>
                    <li><a class="qmparent">My TIPS Page</a></li>
                    <li><a class="qmparent" href="javascript:void(0)">Student Registration</a>
                        <ul>
                            <li><a href="javascript:void(0)">Pre Registration</a></li>
                        </ul>
                    </li>
                    <li><a class="qmparent" href="javascript:void(0)">Student Administration</a>
                        <ul>
                            <li><a href="javascript:void(0)">Admission Approval</a></li>
                            <li><a href="javascript:void(0)">Section Grouping</a></li>
                            <li><a href="javascript:void(0)">Promotion</a></li>
                            <li><a href="javascript:void(0)">Transfer/Discontinue</a></li>
                        </ul>
                    </li>
                    <li><a class="qmparent" href="CallManagement">Call Management</a>
                        <ul>
                            <li><a href="CallRegister">Call Register</a></li>
                            <li><a href="CallForward">Call Forward</a></li>
                        </ul>
                    </li>
                    <li><a class="qmparent" href="javascript:void(0)">Transport Management</a></li>
                    <li><a class="qmparent" href="javascript:void(0)">Boarding</a></li>
                    <li><a class="qmparent" href="javascript:void(0)">Asset Management</a></li>
                    <li><a class="qmparent" href="javascript:void(0)">System Management</a>
                        <ul>
                            <li><a class="qmparent" href="javascript:void(0)">System Settings</a></li>
                            <li><a class="qmparent" href="javascript:void(0)">User Management</a></li>
                        </ul>
                    </li>
                    <li><a class="qmparent" href="javascript:void(0)">Reports</a></li>
                    <li><a class="qmparent" href="javascript:void(0)">Dashboard</a></li>
                    <li class="qmclear">&nbsp;</li>
                </ul>
            </div>
        <div class="main">
            @RenderBody()
        </div>
        <div class="gradient" style="height: 5px; width: 950px; background-color: Gray;">
        </div>
        <div class="footer">
            <p style="font-style: italic; font-size: smaller; font-weight: bold; text-align: left;
                font-variant: normal;">
                Copyright TIPS, 2012, All rights reserved</p>
        </div>
    </div>
</body>
</html>
